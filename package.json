    "lint:fix": "eslint {src,codegen}/**/*.{ts,tsx,js,jsx} --fix",
    "postgen": "eslint -f ./eslint.formatter.js {src,codegen}/**/*.{ts,tsx,js,jsx} --fix || exit 0",
    "serve": "gatsby serve",
    "start": "npm run develop"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.21",
    "@mdx-js/react": "^1.6.21",
    "chalk": "^4.1.0",
